{
  "name": "dyy_ui",
  "author": {
    "name": "dyywork"
  },
  "version": "0.1.40",
  "description": "vue 快捷组件",
  "keywords": [
    "vue",
    "element",
    "vxe-table",
    "ui"
  ],
  "main": "lib/dyyVueComponents.umd.min.js",
  "license": "MIT",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name DUI --dest lib package/components/index.js && npm run lib:all",
    "lib:all": "webpack --config build/webpack.components.js",
    "build:pages": "git subtree push --prefix dist origin gh-pages",
    "delete:pages": "git push origin --delete gh-pages",
    "prepare": "husky install",
    "publish:local": "npm run build &&  git add . && git-cz && npm run delete:pages && npm run build:pages && npm run rm:dist",
    "commit": "git add . && git-cz && git push",
    "rm:dist": "git rm -r dist && git add . && git commit -m 'feat:docs' && git push",
    "report": "vue-cli-service build --report"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "1.0.2",
    "core-js": "^3.6.5",
    "dingyongya-vue-markdown-loader": "^2.5.8",
    "dyy-vue-components": "^0.1.1",
    "element-ui": "^2.15.8",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^9.11.0",
    "html-loader": "^3.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-loader": "^8.0.0",
    "prismjs": "^1.28.0",
    "register-service-worker": "^1.7.1",
    "unescape-html": "^1.1.0",
    "vue": "^2.6.11",
    "vue-markdown-loader": "^2.5.0",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-pwa": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "babel-plugin-prismjs": "^2.1.0",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "css-loader": "^2.1.0",
    "cz-customizable": "^6.3.0",
    "dayjs": "^1.11.2",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^8.0.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass": "^1.52.3",
    "sass-loader": "^10.2.1",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-template-compiler": "^2.6.14",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0",
    "webpackbar": "^5.0.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
